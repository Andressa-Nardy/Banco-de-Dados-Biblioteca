CREATE TABLE EVENTOS (
    ID_Evento SERIAL PRIMARY KEY,
    Nome_Evento VARCHAR(100) NOT NULL,
    Data_Inicio DATE,
    Data_Fim DATE,
    Tipo VARCHAR(50),
    Obras_Relacionadas VARCHAR(255),
    ID_Infra INT,
    CONSTRAINT FK_Evento_Infra FOREIGN KEY (ID_Infra)
        REFERENCES INFRAESTRUTURA(ID_Infra)
);
