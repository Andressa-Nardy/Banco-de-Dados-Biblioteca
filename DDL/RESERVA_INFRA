CREATE TABLE RESERVA_INFRA (
    ID_Infra INT,
    CPF_Usuario VARCHAR(11),
    Data_Reserva_Inicio DATE NOT NULL,
    Data_Reserva_Fim DATE,
    PRIMARY KEY (ID_Infra, CPF_Usuario, Data_Reserva_Inicio),
    CONSTRAINT FK_ReservaInfra_Infra FOREIGN KEY (ID_Infra)
        REFERENCES INFRAESTRUTURA(ID_Infra),
    CONSTRAINT FK_ReservaInfra_Usuario FOREIGN KEY (CPF_Usuario)
        REFERENCES USUARIO(CPF)
);
