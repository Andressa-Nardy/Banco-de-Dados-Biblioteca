CREATE TABLE ACESSO_DIGITAL (
    ID_Item_Digital INT,
    CPF_Usuario VARCHAR(11),
    Data_Acesso DATE DEFAULT CURRENT_DATE,
    PRIMARY KEY (ID_Item_Digital, CPF_Usuario, Data_Acesso),
    CONSTRAINT FK_AcessoDigital_Item FOREIGN KEY (ID_Item_Digital)
        REFERENCES ITEM_DIGITAL(ID_Item),
    CONSTRAINT FK_AcessoDigital_Usuario FOREIGN KEY (CPF_Usuario)
        REFERENCES USUARIO(CPF)
);
